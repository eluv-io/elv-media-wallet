{
  "name": "elv-media-wallet",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/eluv-io/elv-media-wallet",
  "scripts": {
    "bump-version": "npm --git-tag-version --no-commit-hooks version patch",
    "serve": "webpack-dev-server --hot --port 8090 --host=0.0.0.0 --disable-host-check --useLocalIp",
    "serve-https": "webpack-dev-server --https --cert https/private.pem --key https/private.key --hot --port 8090 --host=0.0.0.0 --disable-host-check --useLocalIp",
    "serve-client-test": "webpack-dev-server --hot --entry ./client/test/index.js --port 8091 --host=0.0.0.0 --disable-host-check --useLocalIp",
    "build": "npm run build-app && npm run build-client-test && npm run build-docs",
    "build-app": "rm -rf dist/* && NODE_ENV=production node ./node_modules/webpack-cli/bin/cli.js -p --devtool none",
    "build-client-test": "rm -rf ./client/test/dist/* && NODE_ENV=production node ./node_modules/webpack-cli/bin/cli.js --entry ./client/test/index.js --output-path ./client/test/dist --devtool none",
    "build-docs": "rm -rf ./docs/* && ./node_modules/.bin/jsdoc --configure ./.jsdoc.json --readme README.md",
    "add-build": "COMMIT=`git rev-parse HEAD` ; echo $COMMIT > dist/version.txt ; git add dist",
    "lint": "npm run fix-scss ; npm run lint-scss ; npm run fix-js",
    "lint-show": "npm run lint-scss ; npm run lint-js",
    "lint-js": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "lint-scss": "./node_modules/sass-lint/bin/sass-lint.js -c .scss-lint.yml -v -q",
    "fix-js": "npm run lint-js -- --fix",
    "fix-scss": "./node_modules/sass-lint-auto-fix/dist/index.js -c .scss-lint.yml"
  },
  "pre-commit": {
    "run": [
      "lint",
      "build",
      "add-build"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@auth0/auth0-react": "^1.6.0",
    "@eluvio/elv-client-js": "^3.1.56",
    "jsdoc": "^3.6.7",
    "mobx": "^6.3.2",
    "mobx-react": "^6.3.1",
    "pre-commit": "^1.2.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-inlinesvg": "^2.3.0",
    "react-router-dom": "^5.2.0",
    "urijs": "^1.19.7",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/plugin-transform-regenerator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "autoprefixer": "^9.8.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-preset-mobx": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^2.0.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "regenerator-runtime": "^0.12.1",
    "sass-lint": "^1.12.1",
    "sass-lint-auto-fix": "^0.21.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.22.1",
    "svg-inline-loader": "^0.8.2",
    "svg-inline-react": "^3.1.0",
    "uuid": "^7.0.3",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "babel-preset-mobx"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime"
    ]
  }
}
